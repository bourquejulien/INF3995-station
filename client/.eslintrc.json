{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "plugins": [
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["tsconfig.json"],
        "createDefaultProgram": true

    },
    "rules": {
    },
    "settings": {
        "import/parser": {
            "@typescript-eslint/parser": [".ts", ".tsx"]
        }
    }
}
